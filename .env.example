# Example Environment Configuration
# Copy this file to .env.development or .env.production and update values

# API Configuration
VITE_API_BASE_URL=https://your-api-url.com/api/v4
VITE_USE_MOCK_DATA=false

# Feature Flags
VITE_ENABLE_IFRAME_COMMUNICATION=true
VITE_ENABLE_DEBUG_MODE=false

# Plaid Configuration
VITE_PLAID_ENV=sandbox

# App Configuration
VITE_APP_NAME=Banker Data
VITE_APP_VERSION=1.0.0

# Note: All environment variables must be prefixed with VITE_ to be accessible in the browser